{
    "compilerOptions": {
        "target": "es5",
        "lib": ["es5", "dom"],
        "strict": true,
        "module": "commonjs",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true,
        "paths": {
            "cypress/*": ["cypress/*"]
        },
        "types": ["cypress", "node", "cypress-real-events", "cypress-plugin-api"],
        "baseUrl": "."
    },
    "include": ["cypress/**/*.ts", "scriptMapeamentoTestes.ts", "commitlint.config.ts"],
    "exclude": ["cypress.config.ts", "node_modules", "cypress-real-events"]
}
